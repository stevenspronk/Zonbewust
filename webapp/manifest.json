{
	"_version": "1.5.0",
	"sap.app": {
		"id": "Zonbewust",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "Zonbewust.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Zonbewust.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"targets": {
				"login": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Login"
				},
				"leaderboard": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Leaderboard"
				},
				"gamescreen": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "GameScreen"
				}
			},
			"routes": [{
				"name": "login",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["login"]
			}, {
				"name": "leaderboard",
				"pattern": "leaderboard",
				"titleTarget": "",
				"greedy": false,
				"target": ["leaderboard"]
			}, {
				"name": "gamescreen",
				"pattern": "gamescreen",
				"titleTarget": "",
				"greedy": false,
				"target": ["gamescreen"]
			}],
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "Zonbewust.view",
				"viewLevel": 1,
				"controlId": "App",
				"controlAggregation": "pages",
				"transition": "show"
			}
		}
	}
}