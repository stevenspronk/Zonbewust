{
	"_version": "1.5.0",
	"sap.app": {
		"id": "Zonbewust",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"Z_user_cds": {
				"uri": "/sap/opu/odata/sap/Z_user_cds/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": []
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "Zonbewust.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Zonbewust.i18n.i18n"
				}
			},
			"oUsers": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Z_user_cds",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"targets": {
				"login": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Login",
					"viewLevel": 1
				},
				"leaderboard": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Leaderboard",
					"viewLevel": 2
				},
				"gamescreen": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "GameScreen",
					"viewLevel": 2
				},
				"addaccount": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "AddAccount",
					"viewLevel": 2
				}
			},
			"routes": [{
				"name": "login",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["login"]
			}, {
				"name": "leaderboard",
				"pattern": "leaderboard",
				"titleTarget": "",
				"greedy": false,
				"target": ["leaderboard"]
			}, {
				"name": "gamescreen",
				"pattern": "gamescreen/{name}",
				"titleTarget": "",
				"greedy": false,
				"target": ["gamescreen"]
			}, {
				"name": "addaccount",
				"pattern": "addaccount",
				"titleTarget": "",
				"greedy": false,
				"target": ["addaccount"]
			}],
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "Zonbewust.view",
				"viewLevel": 1,
				"controlId": "App",
				"controlAggregation": "pages",
				"transition": "show"
			}
		}
	}
}